# 网络请求
  pod 'AFNetworking'

# 提示框
  pod 'MBProgressHUD'
 
# 键盘
  pod 'IQKeyboardManager'
 
# 刷新
  pod 'MJRefresh'

# 轮播图
  pod 'SDCycleScrollView','~> 1.3'
  
# jpush
  pod 'JPush'
  
# RAC
  pod 'ReactiveCocoa','~>2.1.8'
  
# 微信
pod 'WechatOpenSDK'

# 布局工具
pod 'Masonry'

# 音频缓存播放
pod 'FreeStreamer', '~> 3.7.2'

end


